// create Jenskins pipeline
pipeline {
    agent any
    stages {
        // build stage
        stage('Build') {
            steps {
                sh 'echo "Hello World"'
                
            }
        }
        // test stage
        stage('Test') {
            steps {
                sh 'echo "Hello World"'
            }
        }
        // deploy stage
        stage('Deploy') {
            steps {
                sh 'echo "Hello World"'
            }
        }


        


    }
}




